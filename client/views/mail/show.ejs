<!DOCTYPE html>
<% var keys = Object.keys(result); %>

<html lang="en">
<head>
</head>
<body >
    <table>
      <tbody>
        <% for (var i = 0; i < keys.length; i++) { %>
          <tr>
          <td><%= keys[i] %></td>
          <td><%= result[keys[i]] %></td>
          </tr>
          <% } %>
        <tr style="margin: auto; text-align: center">
          <td>
            <a href="/mail/">Index </a>
          </td>
          <td>
            <a href="/mail/<%=result.id%>/edit">Edit </a>
          </td>
        </tr>
      </tbody>
    </table>
</body>
</html>
